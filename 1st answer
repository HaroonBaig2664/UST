def find( number_of_days ):

    year = int(number_of_days / 365)
    week = int((number_of_days % 365) /7)
    days = (number_of_days % 365) % 7
    print("years = ",year,

          "\nweeks = ",week,

          "\ndays = ",days)

number_of_days = int(input("enter the number "))

find(number_of_days)
